# Flake8 configuration file for code style enforcement

[flake8]
max-line-length = 127
exclude = 
    .git,
    __pycache__,
    docs,
    .venv,
    build,
    dist,
    .eggs,
    *.egg-info
ignore = 
    E203,
    E266,
    E501,
    W503,
    F403,
    F401
per-file-ignores =
    __init__.py:F401
